    "@ethereumjs/config-prettier": "^1.1.1",
    "@ethereumjs/config-tsc": "^1.1.1",
    "@ethereumjs/config-tslint": "^1.1.1",
    "@types/node": "^12.7.2",
    "@types/tape": "^4.2.33",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "tape": "^4.11.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3",
    "typestrict": "^1.0.2"
